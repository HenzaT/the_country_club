<div class="favourite">
  <% favourites.each do |favourite| %>
    <ul>
      <li>Your rating: <%= favourite.rating %></li>
      <li>Notes: <%= favourite.note %></li>
      <li>Visit Date: <%= favourite.visit_date %></li>
      <li>Times visited: <%= favourite.times_visited %></li>
    </ul>
  <% end %>
</div>
